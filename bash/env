# path set for /usr/local
export PATH="/usr/local/bin:/usr/local/sbin:/usr/local/mysql/bin:~/bin:$PATH"

# change default text editor to TextMate
export EDITOR="mate -w"

# not sure what this is :)
SSH_ENV="$HOME/.ssh/environment"

# allows ruby to automatically load rubygems without having to require it in every script
export RUBYOPT=rubygems 

